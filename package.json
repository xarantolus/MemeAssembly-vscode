{
    "name": "memeassembly",
    "version": "0.3.3",
    "publisher": "xarantolus",
    "engines": {
        "vscode": "^1.59.0"
    },
    "license": "SEE LICENSE IN LICENSE",
    "displayName": "MemeAssembly",
    "description": "Syntax highlighting and snippets for MemeAssembly, a meme-based programming language.",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "memeassembly",
        "x86",
        "meme"
    ],
    "repository": {
        "url": "https://github.com/xarantolus/MemeAssembly-vscode"
    },
    "icon": "img/icon-128.png",
    "contributes": {
        "languages": [
            {
                "id": "memeasm",
                "aliases": [
                    "MemeAssembly",
                    "memeasm"
                ],
                "extensions": [
                    "memeasm"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "memeasm",
                "scopeName": "source.memeasm",
                "path": "./syntaxes/memeasm.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "memeasm",
                "path": "./snippets/snippets.code-snippets"
            }
        ],
        "configurationDefaults": {
            "[memeasm]": {
                "editor.wordBasedSuggestions": false,
                "editor.suggest.snippetsPreventQuickSuggestions": true
            }
        },
        "commands": [
            {
                "command": "memeasm.run-file",
                "title": "MemeAssembly: Run current file",
                "enablement": "editorLangId == memeasm"
            }
        ],
        "keybindings": [
            {
                "command": "memeasm.run-file",
                "key": "ctrl+m",
                "when": "editorTextFocus && editorLangId == memeasm"
            }
        ]
    },
    "activationEvents": [
        "onLanguage:memeasm"
    ],
    "main": "out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.7.1",
        "@types/shell-escape": "^0.2.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "@types/vscode": "^1.60.0",
        "oniguruma-ext": "^1.1.27",
        "shell-escape": "^0.2.0",
        "vscode-oniguruma": "^1.5.1",
        "vscode-textmate": "^5.4.0"
    }
}
